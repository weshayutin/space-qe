package com.redhat.rhn.harness.common;

import java.util.ArrayList;

public class SolUnit {
	private String name = null;
	private String solaris = null;
	private String arch = null;
	private String rhn_arch = null;
	private String cluster = null;
	private String patch = null;
	private String hostname = null;
	private String customChannel = null;
	private String customChildChannel = null;
	private String activationKey = null;
	private ArrayList<String> clustFNs = null;
	private ArrayList<String> patchFNs = null;
	
	/**
	 * @return the activationKey
	 */
	public String getActivationKey() {
		return activationKey;
	}

	/**
	 * @param activationKey the activationKey to set
	 */
	public void setActivationKey(String activationKey) {
		this.activationKey = activationKey;
	}

	/**
	 * @return the clustFNs
	 */
	public ArrayList<String> getClustFNs() {
		return clustFNs;
	}

	/**
	 * @param clustFNs the clustFNs to set
	 */
	public void setClustFNs(ArrayList<String> clustFNs) {
		this.clustFNs = clustFNs;
	}

	/**
	 * @return the patchFNs
	 */
	public ArrayList<String> getPatchFNs() {
		return patchFNs;
	}

	/**
	 * @param patchFNs the patchFNs to set
	 */
	public void setPatchFNs(ArrayList<String> patchFNs) {
		this.patchFNs = patchFNs;
	}

	/**
	 * @return the customChannel
	 */
	public String getCustomChannel() {
		return customChannel;
	}

	/**
	 * @param customChannel the customChannel to set
	 */
	public void setCustomChannel(String customChannel) {
		this.customChannel = customChannel;
	}

	/**
	 * @return the customChildChannel
	 */
	public String getCustomChildChannel() {
		return customChildChannel;
	}

	/**
	 * @param customChildChannel the customChildChannel to set
	 */
	public void setCustomChildChannel(String customChildChannel) {
		this.customChildChannel = customChildChannel;
	}

	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * @return the solaris
	 */
	public String getSolaris() {
		return solaris;
	}

	/**
	 * @param solaris the solaris to set
	 */
	public void setSolaris(String solaris) {
		this.solaris = solaris;
	}

	/**
	 * @return the arch
	 */
	public String getArch() {
		return arch;
	}

	/**
	 * @param arch the arch to set
	 */
	public void setArch(String arch) {
		this.arch = arch;
	}

	/**
	 * @return the rhn_arch
	 */
	public String getRhn_arch() {
		return rhn_arch;
	}

	/**
	 * @param rhn_arch the rhn_arch to set
	 */
	public void setRhn_arch(String rhn_arch) {
		this.rhn_arch = rhn_arch;
	}

	/**
	 * @return the cluster
	 */
	public String getCluster() {
		return cluster;
	}

	/**
	 * @param cluster the cluster to set
	 */
	public void setCluster(String cluster) {
		this.cluster = cluster;
	}

	/**
	 * @return the patch
	 */
	public String getPatch() {
		return patch;
	}

	/**
	 * @param patch the patch to set
	 */
	public void setPatch(String patch) {
		this.patch = patch;
	}

	/**
	 * @return the hostname
	 */
	public String getHostname() {
		return hostname;
	}

	/**
	 * @param hostname the hostname to set
	 */
	public void setHostname(String hostname) {
		this.hostname = hostname;
	}

	public SolUnit(){
	}
}
