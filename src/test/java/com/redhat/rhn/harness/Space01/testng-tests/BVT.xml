<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Build Verification Test">
	
	
	
	<test name="BuildVerificationTest">
		<groups>
			<run>
				<include name="setup" />
				<include name="teardown" />
				<include name="tests" />
				<include name="testplan-BVT" />
			</run>
		</groups>
		<classes>
			<class name="com.redhat.rhn.harness.Space01.BVT02" />
			<class name="com.redhat.rhn.harness.common.SeleniumSetup" />
			<class name="com.redhat.qe.auto.selenium.ExtendedSelenium" />
		</classes>
	</test>
	
	 
			
	<listeners>
  		<listener class-name="com.redhat.qe.auto.selenium.TestNGListener" />
	</listeners>
	
	
