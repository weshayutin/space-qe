<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="ConfigManagement">
	
	
	
	
	  <test name="ConfigManagement">
		<groups>
			<run>
				<include name="setup" />
				<include name="teardown" />
				<include name="testplan-ConfigurationManagement" />
			</run>
		</groups>
		<classes>
			<class name="com.redhat.rhn.harness.Space01.ConfigManagement" />
			<class name="com.redhat.rhn.harness.common.SeleniumSetup" />
			<class name="com.redhat.qe.auto.selenium.ExtendedSelenium" />
		</classes>
	</test>  
	
	
	 
		
	<listeners>
  		<listener class-name="com.redhat.qe.auto.selenium.TestNGListener" />
	</listeners>
	
	
